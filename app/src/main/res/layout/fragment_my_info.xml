<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_info_fragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.infraandroid.myinfo.MyInfoFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/my_info_user_profile_iv"
            android:layout_width="71dp"
            android:layout_height="71dp"
            android:src="@drawable/user_photo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="64dp" />
        <ImageView
            android:id="@+id/my_info_bubble_one_iv"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:src="@drawable/bubble_one"
            app:layout_constraintStart_toEndOf="@id/my_info_user_profile_iv"
            app:layout_constraintTop_toBottomOf="@id/my_info_alarm_iv"
            android:layout_marginTop="34dp"
            android:layout_marginStart="88dp"/>
        <ImageView
            android:id="@+id/my_info_bubble_two_iv"
            android:layout_width="11dp"
            android:layout_height="11dp"
            android:src="@drawable/bubble_two"
            app:layout_constraintStart_toEndOf="@id/my_info_user_profile_iv"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="60dp"
            android:layout_marginStart="36dp"/>
        <ImageView
            android:id="@+id/my_info_bubble_three_iv"
            android:layout_width="6dp"
            android:layout_height="6dp"
            android:src="@drawable/bubble_three"
            app:layout_constraintEnd_toStartOf="@id/my_info_user_profile_iv"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="102dp"
            android:layout_marginEnd="100dp"/>
        <ImageView
            android:id="@+id/my_info_alarm_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_alarm"
            app:layout_constraintStart_toEndOf="@id/my_info_user_profile_iv"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="34dp"
            android:layout_marginStart="64dp"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_setting"
            app:layout_constraintStart_toEndOf="@id/my_info_alarm_iv"
            app:layout_constraintTop_toTopOf="@id/my_info_alarm_iv"
            android:layout_marginStart="10dp"/>
        <TextView
            android:id="@+id/my_info_user_name_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/my_info_name"
            android:textColor="@color/black"
            android:textSize="19sp"
            android:fontFamily="@font/noto_sans_kr_bold"
            android:includeFontPadding="false"
            app:layout_constraintTop_toBottomOf="@id/my_info_user_profile_iv"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="6dp" />
        <TextView
            android:id="@+id/my_info_hashtag_one_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="#산업디자이너"
            android:textColor="@color/infra_gray_n"
            android:textSize="13sp"
            android:fontFamily="@font/noto_sans_kr_regular"
            android:includeFontPadding="false"
            app:layout_constraintTop_toBottomOf="@id/my_info_user_name_tv"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/my_info_hashtag_two_tv"
            app:layout_constraintHorizontal_chainStyle="packed"/>
        <TextView
            android:id="@+id/my_info_hashtag_two_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="#3D모델링"
            android:textColor="@color/infra_gray_n"
            android:textSize="13sp"
            android:fontFamily="@font/noto_sans_kr_regular"
            android:includeFontPadding="false"
            app:layout_constraintTop_toBottomOf="@id/my_info_user_name_tv"
            app:layout_constraintStart_toEndOf="@id/my_info_hashtag_one_tv"
            app:layout_constraintEnd_toStartOf="@id/my_info_hashtag_three_tv"
            app:layout_constraintHorizontal_chainStyle="packed"/>
        <TextView
            android:id="@+id/my_info_hashtag_three_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="#3개까지"
            android:textColor="@color/infra_gray_n"
            android:textSize="13sp"
            android:fontFamily="@font/noto_sans_kr_regular"
            android:includeFontPadding="false"
            app:layout_constraintTop_toBottomOf="@id/my_info_user_name_tv"
            app:layout_constraintStart_toEndOf="@id/my_info_hashtag_two_tv"
            app:layout_constraintEnd_toEndOf="parent" />
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/my_info_constraint_layout"
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:background="@drawable/my_info_backgound"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/my_info_hashtag_one_tv"
            android:layout_marginTop="26dp"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="30dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/my_info_my_info_layout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="36dp"
                android:layout_marginTop="24dp">
                <ImageView
                    android:id="@+id/my_info_my_info_iv"
                    android:layout_width="26dp"
                    android:layout_height="26dp"
                    android:src="@drawable/my_info"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:id="@+id/my_info_my_info_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="내 정보"
                    android:textColor="@color/white"
                    android:textSize="11sp"
                    android:fontFamily="@font/noto_sans_kr_medium"
                    android:includeFontPadding="false"
                    app:layout_constraintTop_toBottomOf="@id/my_info_my_info_iv"
                    app:layout_constraintStart_toStartOf="@id/my_info_my_info_iv"
                    app:layout_constraintEnd_toEndOf="@id/my_info_my_info_iv"
                    android:layout_marginTop="8dp"/>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/my_info_intro_page_layout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="@id/my_info_my_info_layout"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent">
                <ImageView
                    android:id="@+id/my_info_intro_page_iv"
                    android:layout_width="26dp"
                    android:layout_height="26dp"
                    android:src="@drawable/intro_page"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:id="@+id/my_info_intro_page_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="소개 페이지"
                    android:textColor="@color/white"
                    android:textSize="11sp"
                    android:fontFamily="@font/noto_sans_kr_medium"
                    android:includeFontPadding="false"
                    app:layout_constraintTop_toBottomOf="@id/my_info_intro_page_iv"
                    app:layout_constraintStart_toStartOf="@id/my_info_intro_page_iv"
                    app:layout_constraintEnd_toEndOf="@id/my_info_intro_page_iv"
                    android:layout_marginTop="8dp"/>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/my_info_bookmark_layout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="@id/my_info_my_info_layout"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="36dp">
                <ImageView
                    android:id="@+id/my_info_bookmark_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_bookmark_white"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:id="@+id/my_info_bookmark_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="스크랩"
                    android:textColor="@color/white"
                    android:textSize="11sp"
                    android:fontFamily="@font/noto_sans_kr_medium"
                    android:includeFontPadding="false"
                    app:layout_constraintTop_toBottomOf="@id/my_info_bookmark_iv"
                    app:layout_constraintStart_toStartOf="@id/my_info_bookmark_iv"
                    app:layout_constraintEnd_toEndOf="@id/my_info_bookmark_iv"
                    android:layout_marginTop="8dp"/>
            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <LinearLayout
            android:id="@+id/my_info_first_linearlayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/my_info_constraint_layout"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="50dp"
            android:layout_marginTop="34dp"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/my_info_my_idea_linearlayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/my_info_my_idea_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_lightbulb" />
                <TextView
                    android:id="@+id/my_info_my_idea_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="내 아이디어 관리"
                    android:textColor="@color/infra_black_b"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_kr_regular"
                    android:includeFontPadding="false"
                    android:layout_marginStart="28dp"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/my_info_open_log_linearlayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="31dp">
                <ImageView
                    android:id="@+id/my_info_open_log_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_browse_his" />
                <TextView
                    android:id="@+id/my_info_open_log_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="열람 기록"
                    android:textColor="@color/infra_black_b"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_kr_regular"
                    android:includeFontPadding="false"
                    android:layout_marginStart="28dp"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/my_info_activity_log_linearlayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="31dp">
                <ImageView
                    android:id="@+id/my_info_activity_log_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_activity_log" />
                <TextView
                    android:id="@+id/my_info_activity_log_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="활동 기록"
                    android:textColor="@color/infra_black_b"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_kr_regular"
                    android:includeFontPadding="false"
                    android:layout_marginStart="28dp"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/my_info_application_status_linearlayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="31dp">
                <ImageView
                    android:id="@+id/my_info_application_status_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_application_status" />
                <TextView
                    android:id="@+id/my_info_application_status_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="프로젝트 신청 현황"
                    android:textColor="@color/infra_black_b"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_kr_regular"
                    android:includeFontPadding="false"
                    android:layout_marginStart="28dp"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/my_info_evaluation_linearlayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="31dp">
                <ImageView
                    android:id="@+id/my_info_evaluation_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_evaluation" />
                <TextView
                    android:id="@+id/my_info_evaluation_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="팀원 평가 관리"
                    android:textColor="@color/infra_black_b"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_kr_regular"
                    android:includeFontPadding="false"
                    android:layout_marginStart="28dp"/>
            </LinearLayout>
        </LinearLayout>
        <View
            android:id="@+id/my_info_line_one_view"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/infra_gray_e"
            app:layout_constraintTop_toBottomOf="@id/my_info_first_linearlayout"
            android:layout_marginHorizontal="30dp"
            android:layout_marginVertical="38dp"/>
        <LinearLayout
            android:id="@+id/my_info_second_linearlayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/my_info_line_one_view"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="38dp"
            android:layout_marginStart="50dp"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/my_info_hashtag_alarm_linearlayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/my_info_hashtag_alarm_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_hashtag_alarm" />
                <TextView
                    android:id="@+id/my_info_hashtag_alarm_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="해시태그 알림"
                    android:textColor="@color/infra_black_b"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_kr_regular"
                    android:includeFontPadding="false"
                    android:layout_marginStart="28dp"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/my_info_interest_linearlayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="31dp">
                <ImageView
                    android:id="@+id/my_info_interests_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_interests" />
                <TextView
                    android:id="@+id/my_info_interests_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="관심 분야 설정"
                    android:textColor="@color/infra_black_b"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_kr_regular"
                    android:includeFontPadding="false"
                    android:layout_marginStart="28dp"/>
            </LinearLayout>
        </LinearLayout>
        <View
            android:id="@+id/my_info_line_two_view"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/infra_gray_e"
            app:layout_constraintTop_toBottomOf="@id/my_info_second_linearlayout"
            android:layout_marginHorizontal="30dp"
            android:layout_marginVertical="38dp"/>
        <LinearLayout
            android:id="@+id/my_info_third_linearlayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/my_info_line_two_view"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="38dp"
            android:layout_marginStart="50dp"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/my_info_user_guide_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_user_guide" />
                <TextView
                    android:id="@+id/my_info_user_guide_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="이용 가이드"
                    android:textColor="@color/infra_black_b"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_kr_regular"
                    android:includeFontPadding="false"
                    android:layout_marginStart="28dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="31dp"
                android:layout_marginBottom="50dp">
                <ImageView
                    android:id="@+id/my_info_customer_sc_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_customer_sc" />
                <TextView
                    android:id="@+id/my_info_customer_sc_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="고객센터"
                    android:textColor="@color/infra_black_b"
                    android:textSize="14sp"
                    android:fontFamily="@font/noto_sans_kr_regular"
                    android:includeFontPadding="false"
                    android:layout_marginStart="28dp"/>
            </LinearLayout>
        </LinearLayout>



    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>