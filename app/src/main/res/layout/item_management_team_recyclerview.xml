<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginStart="31dp"
    android:layout_marginEnd="30dp"
    android:layout_marginBottom="36dp"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintStart_toStartOf="parent">

    <ImageView
        android:id="@+id/team_member_profile_iv"
        android:layout_width="49dp"
        android:layout_height="49dp"
        android:src="@drawable/team_user_profile_example"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
        android:id="@+id/team_member_name_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="외국인인척하는 사람"
        android:fontFamily="@font/noto_sans_kr_regular"
        android:textSize="15sp"
        android:textColor="@color/black"
        android:layout_marginStart="13dp"
        android:includeFontPadding="false"
        app:layout_constraintStart_toEndOf="@+id/team_member_profile_iv"
        app:layout_constraintTop_toTopOf="@+id/team_member_profile_iv" />
    <ImageView
        android:id="@+id/team_member_score_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_yellow_star"
        app:layout_constraintStart_toStartOf="@id/team_member_name_tv"
        app:layout_constraintTop_toBottomOf="@id/team_member_name_tv"
        android:layout_marginTop="8dp"/>
    <TextView
        android:id="@+id/team_member_score_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/noto_sans_kr_regular"
        android:includeFontPadding="false"
        android:text="4.5"
        android:textColor="@color/black"
        android:textSize="13sp"
        android:layout_marginStart="3dp"
        app:layout_constraintStart_toEndOf="@+id/team_member_score_iv"
        app:layout_constraintTop_toTopOf="@id/team_member_score_iv"
        app:layout_constraintBottom_toBottomOf="@+id/team_member_score_iv"/>

    <ImageView
        android:id="@+id/team_member_eval_modify_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_modify"
        android:layout_marginEnd="3dp"
        app:layout_constraintEnd_toStartOf="@+id/team_member_eval_modify_tv"
        app:layout_constraintTop_toTopOf="@+id/team_member_eval_modify_tv"
        app:layout_constraintBottom_toBottomOf="@+id/team_member_eval_modify_tv"/>
    <TextView
        android:id="@+id/team_member_eval_modify_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/noto_sans_kr_medium"
        android:includeFontPadding="false"
        android:text="수정"
        android:textColor="@color/infra_light_gray_d"
        android:textSize="12sp"
        android:layout_marginEnd="30dp"
        app:layout_constraintEnd_toStartOf="@+id/team_member_eval_delete_iv"
        app:layout_constraintTop_toTopOf="@id/team_member_eval_delete_iv"
        app:layout_constraintBottom_toBottomOf="@+id/team_member_eval_delete_iv"/>

    <ImageView
        android:id="@+id/team_member_eval_delete_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_delete_trash"
        android:layout_marginEnd="3dp"
        app:layout_constraintEnd_toStartOf="@+id/team_member_eval_delete_tv"
        app:layout_constraintTop_toTopOf="@+id/team_member_eval_delete_tv"
        app:layout_constraintBottom_toBottomOf="@+id/team_member_eval_delete_tv"/>
    <TextView
        android:id="@+id/team_member_eval_delete_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/noto_sans_kr_medium"
        android:includeFontPadding="false"
        android:text="삭제"
        android:textColor="@color/infra_light_gray_d"
        android:textSize="12sp"
        android:layout_marginBottom="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>


</androidx.constraintlayout.widget.ConstraintLayout>