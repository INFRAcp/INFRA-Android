<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/infra_gray_c"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.example.infraandroid.id.view.SignUpFourthFragment">

    <TextView
        android:id="@+id/sign_up_page_three_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="4/4"
        android:textColor="@color/infra_gray_s"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:fontFamily="@font/noto_sans_kr_bold"
        android:textSize="16sp"
        android:includeFontPadding="false"
        android:layout_marginRight="20dp"
        android:layout_marginTop="16dp"/>

    <TextView
        android:id="@+id/sign_up_fourth_comment_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sign_up_complete_comment"
        android:textColor="@color/infra_blue_f"
        android:fontFamily="@font/leferi_base_bold"
        android:textSize="26sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="104dp"
        android:layout_marginLeft="24dp"/>

    <ImageView
        android:id="@+id/complete_sign_up_image_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_infra_sign_up_compelete"
        app:layout_constraintTop_toBottomOf="@id/sign_up_fourth_comment_text_view"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="130dp"/>


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/go_to_login_page_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/check_same_id_button_enable_true_background"
        android:textColor="@color/white"
        android:text="시작하기"
        style="?android:borderlessButtonStyle"
        android:layout_marginTop="223dp"
        android:layout_marginHorizontal="26dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/complete_sign_up_image_view"
        android:fontFamily="@font/noto_sans_kr_bold"
        android:textSize="16sp"
        android:paddingVertical="4dp" />


</androidx.constraintlayout.widget.ConstraintLayout>